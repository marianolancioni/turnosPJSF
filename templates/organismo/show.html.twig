{% extends 'base.html.twig' %}

{% block title %}Organismo{% endblock %}

{% block body %}
    <h1>Organismo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ organismo.id }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ organismo.codigo }}</td>
            </tr>
            <tr>
                <th>Organismo</th>
                <td>{{ organismo.organismo }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ organismo.telefono }}</td>
            </tr>
            <tr>
                <th>Habilitado</th>
                <td>{{ organismo.habilitado ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('organismo_index') }}">back to list</a>

    <a href="{{ path('organismo_edit', {'id': organismo.id}) }}">edit</a>

    {{ include('organismo/_delete_form.html.twig') }}
{% endblock %}
